<Application
    x:Class="BubbleShot.UniversalApp.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:windows="using:Prism.Unity.Windows"
    xmlns:converters="using:BubbleShot.UniversalApp.Converters"
    xmlns:converters1="using:WinRTXamlToolkit.Converters"
    RequestedTheme="Light">
    <Application.Resources>
        <converters:NullVisibilityConverter x:Key="NullToVisibilityConverter"/>
        <converters1:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"></converters1:BooleanToVisibilityConverter>
        <FontFamily x:Key="FontAwesome">pack://application:,,,/Fonts/fontawesome-webfont.ttf#FontAwesome</FontFamily>
    </Application.Resources>
</Application>
